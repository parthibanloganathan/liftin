{
  "expo": {
    "name": "Liftin",
    "slug": "liftin",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.4",
    "primaryColor": "#EB3349",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.parthi.liftin"
    },
    "android": {
      "package": "com.parthi.liftin",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/foreground.png",
        "backgroundImage": "./assets/background.png"
      },
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "AUDIO_RECORDING"
      ]
    },
    "androidStatusBar": {
      "backgroundColor": "#C81429",
      "barStyle": "light-content"
    }
  }
}