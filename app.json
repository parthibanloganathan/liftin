{
  "expo": {
    "name": "Liftin",
    "slug": "liftin",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.5",
    "primaryColor": "#EB3349",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.parthi.liftin",
      "infoPlist": {
        "NSCameraUsageDescription": "Give Liftin access to your camera so that you can record your sets",
        "NSMicrophoneUsageDescription": "Give Liftin access to the microphone in your camera so that you can record your sets",
        "NSPhotoLibraryAddUsageDescription": "Give Liftin access to your photos library to select recordigns of your sets",
        "NSPhotoLibraryUsageDescription": "Give Liftin access to your photos library to select recordigns of your sets"
      }
    },
    "android": {
      "package": "com.parthi.liftin",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/foreground.png",
        "backgroundImage": "./assets/background.png"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO"
      ]
    },
    "androidStatusBar": {
      "backgroundColor": "#C81429",
      "barStyle": "light-content"
    }
  }
}